    "modulePaths": [
      "/Volumes/macOS Catalina - Data/Users/joshhunt/dev/destinyDataExplorer/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
